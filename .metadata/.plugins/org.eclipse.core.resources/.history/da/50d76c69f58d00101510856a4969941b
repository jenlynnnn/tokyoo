package ch2;

/*
 * 연산자란?
 * 값을 계산하거나 비교할 때 사용하는 기호
 * 
 * */
public class Operation1 {

	public static void main(String[] args) {
		
		// 산술 연산자 : +, -, *, /, %
		int a = 10 + 5;
		int b = 10 - 5;
		int c = 6 * 3;
		int d = 9 / 4;
		int e = 9 % 4;
		
		System.out.println(d);
		System.out.println(e);
		
		// 비교 연산자 : ==, !=, >, <, >=, <=
		
		//논리 연산자 : &&, ||, !
		
		
	}

}
