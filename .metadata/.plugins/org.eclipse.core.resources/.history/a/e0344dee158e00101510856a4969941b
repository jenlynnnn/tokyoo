package exam;

public class Exam1 {

	public static void main(String[] args) {
		
		int apple = 12;
		int year = 2025;
		int money = -1000;
		double length = 9.8;
		double score = 88.5;
		double minusValue = -4.6;
		String food = "떡볶이";
		String city = "서울";
		String hobby = "코딩";
		boolean hasCar = false;
		boolean isDtudent = true;
		boolean isWeekend = false;
		
		int a = 10, b = 3;
		System.out.println(a + b);
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println(a / b);
		System.out.println(a % b);
	}

}
