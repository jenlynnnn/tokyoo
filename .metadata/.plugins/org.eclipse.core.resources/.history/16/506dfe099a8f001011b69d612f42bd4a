package Exam;

import java.util.Iterator;
import java.util.Scanner;

public class day2Ex02 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		// #1
		System.out.println("치킨 수를 입력하세요. : ");
		int c = sc.nextInt();
		for (int i = 1; i <= c; i++)
			System.out.println("치킨 " + i + "마리 주문이요");

		// #2
		System.out.println("스쿼트 몇 번 할까요?");
		int squat = sc.nextInt();
		for (int i = 1; i <= squat; i++)
			System.out.println("스쿼트 " + i + "회 완료");
		System.out.println("오운완");

		// #3
		System.out.println("숫자를 입력하세요. : ");
		int en = sc.nextInt();
		for (int i = 2; i <= en; i += 2) {
			System.out.println(i);
		}

		// #4
		System.out.println("숫자 2개를 입력하세요. : ");
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		for (int i = num1; i <= num2; i++) {
			System.out.println(i);
		}

		// #5 구구단 3단
		System.out.println("구구단 3단");
		for (int i = 1; i < 10; i++) {
			System.out.println("3 * " + i + " = " + (3 * i));
		}

		// #6 입력한 수로 구구단
		System.out.println("단을 입력하세요. : ");
		int gugudan = sc.nextInt();
		for (int i = 1; i < 10; i++) {
			System.out.println(gugudan + " * " + i + " = " + (gugudan * i));
		}

		// #7 숫자 입력 받아 3의 배수일 떄만 "점푸!" 출력
		System.out.println("숫자를 입력하세요. : ");
		int jump = sc.nextInt();
		for (int i = 1; i < jump; i++) {
			if (i % 3 == 0)
				System.out.println("점프!");
			else
				System.out.println("패스");
		}

		// #7 숫자 입력 받아 3의 배수일 떄만 "점푸!" 출력
		System.out.println("숫자를 입력하세요(별). : ");
		int n = sc.nextInt();
		for (int i = 1; i < jump; i++) {
			
		}

	}

}
