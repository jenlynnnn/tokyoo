package exam;

public class Exam1 {

	public static void main(String[] args) {
		
		int apple = 12;
		System.out.println("#1 " + "apple = " + apple);
		
		int year = 2025;
		System.out.println("#2 " + "year = " + year);
		
		int money = -1000;
		System.out.println("#3 " + "money = " + money);
		
		double length = 9.8;
		System.out.println("#4 " + "length = " + length);
		
		double score = 88.5;
		System.out.println("#5 " + "score = " + score);
		
		double minusValue = -4.6;
		System.out.println("#6 " + "minusValue = " + minusValue);
		
		String food = "떡볶이";
		System.out.println("#7 " + "food = " + food);
		
		String city = "서울";
		System.out.println("#8 " + "city = " + city);
		
		String hobby = "코딩";
		System.out.println("#9 " + "hobby = " + hobby);
		
		boolean hasCar = false;
		System.out.println("#10 " + "hasCar = " + hasCar);
		
		boolean isStudent = true;
		System.out.println("#11 " + "isStudent = " + isStudent);
		
		boolean isWeekend = false;
		System.out.println("#12 " + "isWeekend = " + isWeekend);
		
		int a = 10, b = 3;
		System.out.println("#1 " + "a + b = " + (a + b));
		System.out.println("#1 " + "a - b = " + (a - b));
		System.out.println("#1 " + "a * b = " + (a * b));
		System.out.println("#1 " + "a / b = " + (a / b));
		System.out.println("#1 " + "a % b = " + (a % b));
		
		int sum = 12 + 8;
		System.out.println("#2 " + "12 + 8 = " + sum);
		
		int total = 20 / 3;
		System.out.println("#3 " + "20 + 3 = " + total);
		
		int mod = 14 / 5; 
		System.out.println("#4 " + "14 + 5 = " + mod);
		
		System.out.println("5 % 5 = " + (5 % 5)); // 5 / 5의 결과 값은 1 이고 나머지의 값이 0이 나온다.
		
		
		boolean result1;
		if (15 > 20) {
			result1 = true;
		} else {
			result1 = false;
		}
		System.out.println("#12 " + "(15 > 20) = " + result1);
		
		boolean result2;
		if (10 == 10) {
			result2 = true;
		} else {
			result2 = false;
		}
		System.out.println("#13 " + "(10 == 10) = " + result2);
		
		
		boolean result3;
		if (7 <= 3) {
			result3 = true;
		} else {
			result3 = false;
		}
		System.out.println("#14 " + "(7 <= 3) = " + result3);
		
	}

}
