package dya4;

public class Pizza {

	private String name;
	private String size;
	
	
	public Pizza() {
		
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSize() {
		return size;
	}
	public void setSize(String size) {
		this.size = size;
	}
	@Override
	public String toString() {
		return "Pizza [종류=" + name + ", 사이즈=" + size + ", getName()=" + getName() + ", getSize()=" + getSize() + "]";
	}
	
	
}
