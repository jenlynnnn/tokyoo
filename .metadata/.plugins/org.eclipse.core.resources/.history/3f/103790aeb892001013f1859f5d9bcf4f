package ch1;

public class TestMain {

	public static void main(String[] args) {
		
		// 객체(인스턴스) 생성
		// 클래스명 객체명 = new 생성자함수(); 
		TestData ts1 = new TestData();
		System.out.println("te1 : " + ts1);
		System.out.println("te1 : " + ts1.field1);
		
		ts1.field1 = 100;
		System.out.println("ts1 : "+ ts1.field1);
	}

}
